
WindowManager:
    LoginWindow:  
    ShareOffWindow:
    ShareOnWindow:
    MainWindow:
    Mybikepark:
	Basicinformation:
	Pastfeed:
	Bikerent:
	Bikeavailable:
	Successfulrent:
<LoginWindow>:
    name: 'login'
    studentid: StudentID
    password: Password

    FloatLayout:

        Label:
            pos_hint: {'x':0.2, 'y':0.8}
            size_hint: 0.6, 0.1
            text:'WELCOME'
            color: 0.3,0.6,0.7,1

        Label:
            pos_hint: {'x':0.1, 'y':0.6}
            size_hint: 0.3, 0.1
            text:'StudentID'

        TextInput:
            pos_hint: {'x':0.4, 'y':0.6}
            size_hint: 0.5, 0.1
            id: StudentID
            multiline: False

        Label:
            pos_hint: {'x':0.1, 'y':0.5}
            size_hint: 0.3, 0.1
            text: 'Password'

        TextInput:
            pos_hint: {'x':0.4, 'y':0.5}
            size_hint: 0.5, 0.1
            id: Password
            multiline: False

        Button:
            pos_hint: {'x':0.3, 'y':0.2}
            size_hint: 0.4, 0.1
            text:'Login'
            background_color: 0.3,0.4,0.5,1
            on_release:
                root.btn()
                app.root.current = 'shareoff'
                root.manager.transition.direction = 'up'

<ShareOffWindow>:
    name:'shareoff'

    ImageButton:
        source: 'off.png'
        size_hint: 0.4, 0.1
        pos_hint: {'x': 0.05, 'y': 0.85}
        on_release:
            app.root.current = 'shareon'

    FloatLayout:
        Button:
            pos_hint: {'x':0.2, 'y':0.5}
            size_hint: 0.6, 0.1
            text:'Park bike'
            background_color: 0.3,0.4,0.5,1
            on_release:
             
                app.root.current = 'bikepark'
                root.manager.transition.direction = 'up'
        
        Button:
            pos_hint: {'x':0.2, 'y':0.3}
            size_hint: 0.6, 0.1
            text:'About me'
            background_color: 0.3,0.4,0.5,1
			on_release:
             
                app.root.current = 'basicinformation'
                root.manager.transition.direction = 'up'
        Button:
            pos_hint: {'x':0.2, 'y':0.7}
            size_hint: 0.6, 0.1
            text:'Find my bike'
            background_color: 0.3,0.4,0.5,1
			on_release:
                app.root.current = 'pastfeed'
                root.manager.transition.direction = 'up'


